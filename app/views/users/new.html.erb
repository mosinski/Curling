<div class="page-header">  
	<h3><i class="icon-edit-sign"></i> <%= t 'register' %></h3>
</div>

<%= form_for @user, :html => { :multipart => true } do |f| %>
<%= render partial: 'form', locals: { f: f } %>

<div class="actions">
     <button type="submit" class="btn btn-primary">
  	<i class="icon-check"></i> <%= f.label :register, "style"=>"display:inline" %>
     </button>
</div>
<% end %>


